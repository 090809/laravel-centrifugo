{
    "name": "denis660/laravel-centrifugo",
    "type": "library",
    "description": "Centrifugo broadcaster for laravel 5.6-8 and Centrifugo >= 2.8.1",
    "keywords": ["laravel", "centrifugo", "broadcaster"],
    "license": "MIT",
    "authors": [
        {
            "name": "Denis Zakharenko",
            "email": "amx-serv@ya.ru"
        }
    ],
    "homepage": "https://github.com/denis660/laravel-centrifugo",
    "support": {
        "issues": "https://github.com/denis660/laravel-centrifugo/issues",
        "source": "https://github.com/denis660/laravel-centrifugo"
    },
    "require": {
        "php": "^7.3|^8.0",
       "laravel/framework": "^5.6|^6.0|^7.0|^8.0",
        "guzzlehttp/guzzle": "~6.0|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.7",
        "phpunit/phpunit": "^6||^7||^8"
    },
    "autoload": {
        "psr-4": {
            "denis660\\Centrifuge\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "denis660\\Centrifuge\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Centrifugo\\CentrifugoServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
